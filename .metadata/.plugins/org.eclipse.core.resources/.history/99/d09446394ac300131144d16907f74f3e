package com.wayne.bomberman.framework;

public class Position {
	public int x;
	public int y;
	
	public Position(int x, int y) {
		setX(x);
		setY(y);
	}
	
	public int getX() {
		return this.x;
	}
	
	public int getY() {
		return this.y;
	}
	
	public void setX(int x) {
		this.x = x;
	}
	
	public void setY(int y) {
		this.y = y;
	}
}
		
		
		